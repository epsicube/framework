{
  "name": "epsicube/module-hypercore",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "COLANT Alan",
      "email": "contact@alancolant.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-pcntl": "*",
    "epsicube/foundation": "*",
    "epsicube/support": "*"
  },
  "suggest": {
    "epsicube/module-administration": "Some activities and workflows are registered by this module"
  },
  "autoload": {
    "psr-4": {
      "EpsicubeModules\\Hypercore\\": "."
    }
