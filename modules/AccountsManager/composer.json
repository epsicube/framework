{
  "name": "epsicube/module-accounts-manager",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "COLANT Alan",
      "email": "contact@alancolant.com"
    }
  ],
  "require": {
    "php": "^8.3",
    "illuminate/support": "^12.8",
    "illuminate/database": "^12.8",
    "epsicube/support": "*",
    "filament/filament": "^5.1.3"
  },
  "suggest": {
    "epsicube/module-administration": "User can be managed through the administration panel"
  },
  "autoload": {
    "psr-4": {
      "EpsicubeModules\\AccountsManager\\": "."
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "epsicube": {
      "modules": [
        "EpsicubeModules\\AccountsManager\\AccountsManagerModule"
      ],
      "hypercore": {
        "adapters": [
          "EpsicubeModules\\AccountsManager\\AccountsManagerModuleAdapter"
        ]
      }
    }
  }
}
