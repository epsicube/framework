{
    "name": "unigale/foundation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "COLANT Alan",
            "email": "contact@alancolant.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.8"
    },
    "suggest": {
        "unigale/module-execution-platform": "Some activities and workflows are registered by this module"
    },
    "autoload": {
        "psr-4": {
            "UniGaleModules\\MailingSystem\\": "./src"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "unigale": {
            "modules": [
                "UniGaleModules\\MailingSystem\\MailingSystemModule"
            ]
        }
    }
}
